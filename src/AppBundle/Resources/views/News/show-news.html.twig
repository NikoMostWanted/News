{% extends 'base.html.twig' %}
{% block title %}{% trans %}List news{% endtrans %}{% endblock %}
{% block navbar %}<li><p class="navbar-btn"><a class="btn btn-success" href="{{ url('Publish') }}">{% trans %}Public news{% endtrans %}</a></p></li>{% endblock %}
{% block body %}
    {% for type in news %}
        <i>{{ type.id }}</i>
        <i>Title: {{ type.title }}</i>
        <i>Text: {{ type.text }}</i><br/>
    {% endfor %}
{% endblock %}